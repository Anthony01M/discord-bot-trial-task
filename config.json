{
    "VARIABLES": {
        "LOG_LEVEL": "info",
        "BOT_TOKEN": "",
        "DATABASE": {
            "type": "sqlite",
            "mysql": {
                "host": "localhost",
                "port": 3306,
                "database": "database",
                "name": "root",
                "password": "password"
            },
            "sqlite": {
                "path": "database.sqlite"
            }
        }
    },
    "MESSAGES": {
        "ECHO": {
            "type": "text",
            "EMBED": {
                "color": "random",
                "title": "Echo Command",
                "fields": [
                    { "name": "Original Message", "value": "{original}" },
                    { "name": "Reversed Message", "value": "{reversed}" }
                ]
            },
            "TEXT": {
                "content": "Original Message: {original}\nReversed Message: {reversed}"
            }
        },
        "ASSIGNROLE": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "Assign Role",
                "fields": [
                    { "name": "Role Assigned", "value": "{role}" },
                    { "name": "User", "value": "{user}" }
                ]
            },
            "TEXT": {
                "content": "Role Assigned: {role}\nUser: {user}"
            }
        },
        "REMOVEROLE": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "Remove Role",
                "fields": [
                    { "name": "Role Removed", "value": "{role}" },
                    { "name": "User", "value": "{user}" }
                ]
            },
            "TEXT": {
                "content": "Role Removed: {role}\nUser: {user}"
            }
        },
        "RESTOREROLE": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "Restore Role",
                "fields": [
                    { "name": "Role Restored", "value": "{role}" },
                    { "name": "User", "value": "{user}" }
                ]
            },
            "TEXT": {
                "content": "Role Restored: {role}\nUser: {user}"
            }
        },
        "SETWELCOME": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "Set Welcome Message",
                "fields": [
                    { "name": "Message", "value": "{message}" }
                ]
            },
            "TEXT": {
                "content": "Welcome message set to: {message}"
            }
        }
    },
    "ERRORS": {
        "USER_NOT_FOUND": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "User Not Found",
                "fields": [
                    { "name": "User", "value": "{user}" }
                ]
            },
            "TEXT": {
                "content": "User not found: {user}"
            }
        },
        "ROLE_NOT_FOUND": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "Role Not Found",
                "fields": [
                    { "name": "Role", "value": "{role}" }
                ]
            },
            "TEXT": {
                "content": "Role not found: {role}"
            }
        },
        "USER_ALREADY_HAS_ROLE": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "User Already Has Role",
                "fields": [
                    { "name": "User", "value": "{user}" },
                    { "name": "Role", "value": "{role}" }
                ]
            },
            "TEXT": {
                "content": "User already has role: {role}"
            }
        },
        "USER_DOES_NOT_HAVE_ROLE": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "User Does Not Have Role",
                "fields": [
                    { "name": "User", "value": "{user}" },
                    { "name": "Role", "value": "{role}" }
                ]
            },
            "TEXT": {
                "content": "User does not have role: {role}"
            }
        },
        "INVALID_DURATION": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "Invalid Duration",
                "fields": [
                    { "name": "Duration", "value": "{duration}" }
                ]
            },
            "TEXT": {
                "content": "Invalid duration: {duration}"
            }
        },
        "USER_NOT_ENOUGH_MESSAGES": {
            "type": "text",
            "EMBED": {
                "enabled": true,
                "color": "random",
                "title": "User Not Enough Messages",
                "fields": [
                    { "name": "User", "value": "{user}" },
                    { "name": "Messages", "value": "{messages}" }
                ]
            },
            "TEXT": {
                "content": "User does not have enough messages: {messages}"
            }
        }
    }
}